/boot              emmc        /dev/block/platform/sdio_emmc/by-name/KERNEL
/cache             ext4        /dev/block/platform/sdio_emmc/by-name/CACHE
/data              ext4        /dev/block/platform/sdio_emmc/by-name/userdata    length=-16384
/recovery          emmc        /dev/block/platform/sdio_emmc/by-name/RECOVERY
/system            ext4        /dev/block/platform/sdio_emmc/by-name/SYSTEM
/efs               emmc        /dev/block/platform/sdio_emmc/by-name/efs         flags=display="EFS";backup=1
/wmodem            emmc        /dev/block/platform/sdio_emmc/by-name/MODEM       flags=display="Modem";backup=1
/persistent        emmc        /dev/block/platform/sdio_emmc/by-name/PERSISTENT  flags=display="Persistent";backup=1
/preload           ext4        /dev/block/platform/sdio_emmc/by-name/HIDDEN      flags=display="Hidden";backup=1
/external_sdcard   auto        /dev/block/mmcblk1p1 /dev/block/mmcblk1           flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg           auto        /dev/block/sda1    /dev/block/sda                 flags=display="USB-OTG";storage;wipeingui;removable
